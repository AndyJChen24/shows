{
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
  },
  "extends": [
      "airbnb-base",
      "plugin:react/recommended"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "ecmaFeatures": {
        "jsx": true
      }
  },
  "rules": {
      "linebreak-style": ["error", "unix"],
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error"
  },
  "plugins": [
    "react"
  ]
}